Grammar

    0 $accept: program $end

    1 program: statement_list

    2 statement_list: statement
    3               | statement_list statement

    4 statement: declaration
    5          | attribution
    6          | if_statement
    7          | loop_statement
    8          | return_statement
    9          | function

   10 declaration: type id_list ';'
   11            | type function
   12            | type attribution

   13 type: TYPE

   14 id_list: identifier
   15        | id_list ',' identifier

   16 identifier: ID
   17           | ID '[' NUM ']'

   18 function: ID '(' parameter_list_opt ')' '{' statement_list '}'
   19         | ID '(' parameter_list_opt ')' ';'

   20 parameter_list_opt: parameter_list
   21                   | ε

   22 parameter_list: parameter
   23               | parameter_list ',' parameter

   24 parameter: type ID
   25          | expression

   26 attribution: identifier '=' expression ';'
   27            | identifier ASSIGN_OP expression
   28            | identifier INC_DEC_OP

   29 if_statement: IF '(' comparison ')' '{' statement_list '}'
   30             | IF '(' comparison ')' '{' statement_list '}' ELSE '{' statement_list '}'
   31             | IF '(' comparison ')' '{' statement_list '}' ELSE if_statement

   32 comparison: expression RELATIONAL_OP expression
   33           | comparison LOGICAL_OP comparison
   34           | identifier
   35           | function

   36 loop_statement: WHILE '(' comparison ')' '{' statement_list '}'

   37 return_statement: RETURN expression ';'
   38                 | RETURN ';'

   39 expression: NUM
   40           | STRING_LITERAL
   41           | identifier
   42           | expression '+' expression
   43           | expression '-' expression
   44           | expression '*' expression
   45           | expression '/' expression
   46           | expression BITWISE_OP expression
   47           | identifier INC_DEC_OP
   48           | PREPROCESSOR_OP expression
   49           | UNARY_OP expression
   50           | expression TERNARY_OP expression ':' expression
   51           | '(' expression ')'
   52           | '{' expression_list '}'
   53           | function

   54 expression_list: expression
   55                | expression_list ',' expression


